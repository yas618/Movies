generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
}

model Movie {
    id        Int      @id @default(autoincrement())
   title      String
    description String
    duration     Int
    genre       String
    rating      Decimal
    available  Boolean
    createdAt DateTime @default(now())
  

    @@map("movies")
}
